<h1><%= @game.name %></h1>
<h3>Category: <%= @game.type %></h3>

<h3>Sessions: </h3>

<div id="session-wrapper">
  <% for session in @sessions %>

  <div class="session">
    <h3><a href="/sessions/<%= session.id %>"><%= session.date %></a></h3>
    <p>Game: <%= session.game %></p>
    <p>Start Time: <%= session.start_time %></p>
    <p>End Time: <%= session.end_time %></p>
  </div>

  <% end %>
</div>

<h3>Played in <%= @game.sessions %> sessions</h3>

<h3>Last played on ruby-date-method-for-game by method-show-players-in-last-session</h3>