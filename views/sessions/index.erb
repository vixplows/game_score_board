<h1>Sessions</h1>

<a href="/sessions/new">Add New Session</a>

<div id="session-wrapper">
  <% for session in @sessions %>

  <div class="session">
    <h3><a href="/sessions/<%= session.id %>"><%= session.date %> : <%= session.game %></a></h3>
    <p>Start Time: <%= session.start_time %></p>
    <p>End Time: <%= session.end_time %></p>
  </div>

  <% end %>
</div>